    <script>
      const slider = document.getElementById('slider');
      
      document.getElementById('monthlyBtn').addEventListener('click', function() {
        // Adjust slider width for Premium Plan
        slider.style.width = '190px';
        // Move slider to Premium Plan position
        slider.style.transform = 'translateX(0)';
        
        // Update text colors
        this.style.color = 'white';
        document.getElementById('yearlyBtn').style.color = 'white';
        
        // Show/hide content
        const premiumContent = document.getElementById('premium-plan-content');
        const freeContent = document.getElementById('free-plan-content');
        
        premiumContent.classList.remove('hidden');
        freeContent.classList.remove('active');
        
        // Reset and setup animations after content is displayed
        setTimeout(() => {
          const features = document.querySelectorAll('.feature');
          features.forEach(feature => feature.classList.remove('animate'));
          setupFeatureAnimations();
        }, 100);
      });

      document.getElementById('yearlyBtn').addEventListener('click', function() {
        // Adjust slider width for Free Plan
        slider.style.width = '103px';
        // Move slider to Free Plan position
        slider.style.transform = 'translateX(185px)';
        
        // Update text colors
        this.style.color = 'white';
        document.getElementById('monthlyBtn').style.color = 'white';
        
        // Show/hide content
        const premiumContent = document.getElementById('premium-plan-content');
        const freeContent = document.getElementById('free-plan-content');
        
        premiumContent.classList.add('hidden');
        freeContent.classList.add('active');
        
        // Reset and setup animations after content is displayed
        setTimeout(() => {
          const features = document.querySelectorAll('.feature');
          features.forEach(feature => feature.classList.remove('animate'));
          setupFeatureAnimations();
        }, 100);
      });

      // Feature animation functionality
      function setupFeatureAnimations() {
        const features = document.querySelectorAll('.feature');
        
        const observer = new IntersectionObserver((entries) => {
          entries.forEach((entry, index) => {
            if (entry.isIntersecting) {
              // Add delay based on index for sequential animation
              setTimeout(() => {
                entry.target.classList.add('animate');
              }, index * 200); // 200ms delay between each feature
              
              // Unobserve after animation
              observer.unobserve(entry.target);
            }
          });
        }, {
          threshold: 0.2 // 20% of the element needs to be visible
        });

        features.forEach(feature => {
          observer.observe(feature);
        });
      }

      // Initial setup
      setupFeatureAnimations();
    </script>
  