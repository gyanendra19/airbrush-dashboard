<script>
    document.addEventListener('DOMContentLoaded', function() {
        // Handle inspiration boxes on mobile
        function handleMobileBoxes() {
            const boxes = document.querySelectorAll('.inspiration-box');
            if (window.innerWidth <= 768) {
                boxes.forEach((box, index) => {
                    if (index >= 3) {
                        box.classList.add('hidden-mobile');
                    } else {
                        box.classList.remove('hidden-mobile');
                    }
                });
            } else {
                boxes.forEach(box => {
                    box.classList.remove('hidden-mobile');
                });
            }
        }

        // Call on load
        handleMobileBoxes();

        // Call on resize
        let resizeTimer;
        window.addEventListener('resize', () => {
            clearTimeout(resizeTimer);
            resizeTimer = setTimeout(handleMobileBoxes, 250);
        });

        // Get all slider containers
        const sliderContainers = document.querySelectorAll('.top-slider-container, .bottom-slider-container');
        
        sliderContainers.forEach(container => {
            const images = container.querySelectorAll('.slider-image');
            
            images.forEach(img => {
                // Skip if already wrapped
                if (img.parentElement.classList.contains('slider-image-container')) return;
                
                // Get the animation class from the image
                const animationClass = Array.from(img.classList)
                    .find(className => className.includes('-image-'));
                
                const wrapper = document.createElement('div');
                wrapper.className = 'slider-image-container';
                
                const overlay = document.createElement('div');
                overlay.className = 'image-overlay';
                overlay.innerHTML = `
                    <button class="use-prompt-btn">
                        <div class="icon-circle">
                            <svg width="20" height="20" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M18 13V19C18 19.5304 17.7893 20.0391 17.4142 20.4142C17.0391 20.7893 16.5304 21 16 21H5C4.46957 21 3.96086 20.7893 3.58579 20.4142C3.21071 20.0391 3 19.5304 3 19V8C3 7.46957 3.21071 6.96086 3.58579 6.58579C3.96086 6.21071 4.46957 6 5 6H11" stroke="black" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                <path d="M15 3H21V9" stroke="black" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                <path d="M10 14L21 3" stroke="black" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                            </svg>
                        </div>
                        Use Prompt
                    </button>
                `;
                
                // Insert wrapper before the image
                container.insertBefore(wrapper, img);
                // Move the image into the wrapper
                wrapper.appendChild(img);
                // Add the overlay
                wrapper.appendChild(overlay);

                // Add click event listener to the button
                const usePromptBtn = overlay.querySelector('.use-prompt-btn');
                usePromptBtn.addEventListener('click', handlePrompt);
            });
        });
    });

    // Function to handle Use Prompt button click
    function handlePrompt(event) {
        // Get the prompt from the image's data-prompt attribute
        const img = event.target.closest('.slider-image-container').querySelector('img');
        const prompt = img.getAttribute('data-prompt');
        
        // Store the prompt in localStorage
        localStorage.setItem('selectedPrompt', prompt);
        
        // Find the text-to-image section and scroll to it
        const textToImageSection = document.getElementById('text-to-image');
        if (textToImageSection) {
            textToImageSection.scrollIntoView({ behavior: 'smooth' });
            
            // Set the textarea value
            const textarea = textToImageSection.querySelector('.prompt-input textarea');
            if (textarea) {
                textarea.value = prompt;
                // Trigger input event to ensure any listeners are notified
                textarea.dispatchEvent(new Event('input', { bubbles: true }));
            }
        }
    }
</script>