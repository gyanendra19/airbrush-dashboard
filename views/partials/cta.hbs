  <script>
    document.addEventListener("DOMContentLoaded", () => { const cards =
    document.querySelectorAll(".cta-box"); const observer = new
    IntersectionObserver( (entries) => { entries.forEach((entry) => { if
    (entry.isIntersecting) { entry.target.classList.add("in-view"); } else {
    entry.target.classList.remove("in-view"); } }); }, { threshold: 0.1 } );
    cards.forEach((card) => observer.observe(card)); });
  </script>
  <script>
    document.addEventListener("DOMContentLoaded", () => { const cards =
    document.querySelectorAll(".cta-card"); let currentIndex = 0; const
    totalCards = cards.length; const shuffleInterval = 3000; const cardDelay =
    1000; const resetCards = () => { cards.forEach(card => {
    card.classList.remove("card-visible", "card-hidden", "card-up",
    "card-down"); card.classList.add("card-hidden"); }); }; const showCard =
    (index) => { const card = cards[index];
    card.classList.remove("card-hidden"); card.classList.add("card-visible"); };
    const animateCards = () => { resetCards(); showCard(currentIndex);
    setTimeout(() => { cards[currentIndex].classList.add("card-hidden");
    currentIndex = (currentIndex + 1) % totalCards; showCard(currentIndex); },
    shuffleInterval); setInterval(() => {
    cards[currentIndex].classList.add("card-hidden"); currentIndex =
    (currentIndex + 1) % totalCards; showCard(currentIndex); }, shuffleInterval
    + cardDelay); }; animateCards(); });

